{
  "name": "front-end-starter",
  "version": "6.0.0",
  "description": "General front-end project boilerplate",
  "author": "Renaud Gillet <gillet.ren@gmail.com>",
  "source": "index.html",
  "license": "MIT",
  "scripts": {
    "compile": "sass --watch assets/scss:assets/css --no-source-map",
    "compile:release": "sass --style=compressed assets/scss:assets/css",
    "livereload": "browser-sync start --proxy https://github.com/gilren/front-end-starter --serveStatic assets  --files **/* --no-notify --no-ui",
    "dev": "concurrently \"npm run compile\" \"npm run vite\"",
    "vite": "vite"
  },
  "devDependencies": {
    "bootstrap": "^5.3.0",
    "browser-sync": "^2.27.11",
    "concurrently": "^8.1.0",
    "sass": "^1.58.3"
  },
  "dependencies": {
    "vite": "^4.4.9"
  }
}
